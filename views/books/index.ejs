<h2>Search Books</h2>
<form action="/books" method="GET">
  <div>
    <label>Title</label>
    <input type="text" name="title" value="<%= searchOptions.title %>">
  </div>
  <div>
    <label>Published After</label>
    <input type="date" name="publihedAfter" value="<%= searchOptions.publihedAfter %>">
  </div>
  <div>
    <label>Published Before</label>
    <input type="date" name="publihedBefore" value="<%= searchOptions.publihedBefore %>">
  </div>
  <button type="submit">Search</button>
</form>
<div>
  <% books.forEach(book=> { %>
    <div>
      <%= book.title %>
        <%= book %>
    </div>
    <!-- <img height="150" width="100" src="<%= book.coverImagePath %>"> -->
    <% }) %>
</div>